<h1><%=title%></h1>
<div id="todos">데이터 없음</div>
<!--할일ㅇ 목록 템플릿-->
<script id="temp" type="x-handlebars-template">
  <table class="table table-striped table-hover">
  
      {(#each .)}
         <tr>
             <td>{{completed}}</td>
             <td>{{id}}</td>
             <td>{{title}}</td>
         </tr>
      {{/each}}
 </table>

</script>


<script>
  getTodos(); //함수호출//
  function getTodos() {//함수 정의 --함수이름은 내마음~//
  fetch('https://jsonplaceholder.typicode.com/todos')
  
      .then(response => response.json()) //response.json  response를 json타입으로 바꿔주는 것
      .then(json => {
        console.log(json);
        const temp=Handlebars.compile($("#temp").html());
        $("#todos").html(temp(json))
      })
    }
  </script>
  
